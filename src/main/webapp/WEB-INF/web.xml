<web-app xmlns="http://java.sun.com/xml/ns/javaee"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
      version="3.0">
    
<display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>simple</servlet-name>
        <servlet-class>fr.adhoc.myWebbApp.web.SimpleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listeUt</servlet-name>
        <servlet-class>fr.adhoc.myWebbApp.web.UtilisateurListe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>infosUt</servlet-name>
        <servlet-class>fr.adhoc.myWebbApp.web.UtilisateurInfos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>listeUt</servlet-name>
        <url-pattern>/listeUt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>infosUt</servlet-name>
        <url-pattern>/infosUt</url-pattern>
    </servlet-mapping>

</web-app>